# --- LLM CONFIGURATION ---
# OpenRouter API Key - Get yours at https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_key_here

# --- VOICE CONFIGURATION (LIVEKIT AGENTS) ---
# Deepgram API Key (STT) - Get yours at https://console.deepgram.com/
DEEPGRAM_API_KEY=your_deepgram_key_here

# TTS Provider - Use 'qwen' for local GPU, 'cartesia' or 'openai' for cloud
TTS_TYPE=qwen

# Cartesia API Key (TTS) - Get yours at https://play.cartesia.ai/
CARTESIA_API_KEY=your_cartesia_key_here

# --- DATABASE / MEMORY CONFIGURATION ---
# Supabase Project Details - Get yours at https://supabase.com/
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# --- DASHBOARD CONFIGURATION ---
# These are exposed to the frontend (Vite)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# --- LIVEKIT CLOUD CONFIGURATION ---
# Get these from your LiveKit Cloud Project Settings (https://cloud.livekit.io/)
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret
